pipeline {
   agent any
  triggers{
       git push()
  }
   stages {
   
      stage('Example') {
         steps {
         	// Call the getChangeset function
         	githubChangeset()
         	
            // Call the helloWorld() method and pass a value
            helloWorld("Craig")
            echo "hello raj"
            echo "hello"
         }
       }
   }
}
